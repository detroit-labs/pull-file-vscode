{
    "name": "pull-file",
    "displayName": "Pull File",
    "description": "This extension is used to overwrite the content of the current file with the content of a selected file.",
    "version": "1.0.4",
    "publisher": "shawn-mallon",
    "author": "Shawn Mallon <shawn.mallon@detroitlabs.com>",
    "homepage": "https://github.com/Fieryava/pull-file",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Fieryava/pull-file/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Fieryava/pull-file.git"
    },
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "copy",
        "file"
    ],
    "activationEvents": [
        "onCommand:extension.pullFile"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.pullFile",
                "title": "Pull File"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
